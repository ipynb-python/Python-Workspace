bash .profile/auto-commit-all.sh -h
nohup bash /workspaces/$RepositoryName/.profile/auto-commit-all.sh &
cat ~/.bashrc_tmp 
ls
cat nohup.out 
cat /tmp/auto-commit-workspaces_Python-Workspace.log
nohup bash /workspaces/$RepositoryName/.profile/auto-commit-all.sh 2>&1 &
cat nohup.out 
cat /tmp/auto-commit-workspaces_Python-Workspace.log
cat /tmp/nohup.out 
ls -l /workspaces/Python-Workspace/.profile/auto-commit-all.sh
chmod a+x /workspaces/Python-Workspace/.profile/auto-commit-all.sh
